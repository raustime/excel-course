{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "require-jsdoc":"off",
        "indent":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "linebreak-style": ["error", "windows"]
    },
    "extends": [
        "eslint:recommended", "google"
    ]
    
}